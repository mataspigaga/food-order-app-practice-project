Ctx.items.length === 0) {\n      return;\n    }\n    setBtnIsHighlighted(true);\n\n    const timer = setTimeout(() => {\n      setBtnIsHighlighted(false);\n    }, 300);\n\n    return () => {\n      clearTimeout(timer);\n    };\n  }, [items]);\n\n  return (\n    <button className={btnClasses} onClick={props.onClick}>\n      <span className={classes.icon}>\n        <CartIcon />\n      </span>\n      <span>Your Cart</span>\n      <span className={classes.badge}>{numberOfCartItems}</span>\n    </button>\n  );\n};\n\nexport default HeaderCartButton;\n","/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/UI/Modal.js",[],"/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/UI/Card.js",[],"/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/Cart/CartIcon.js",[],"/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/Meals/MealItem/MealItem.js",[],"/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/Meals/MealItem/MealItemForm.js",[],"/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/UI/Input.js",[],"/Users/mataspigaga/Desktop/React Complete Guide/PracticeProjects/food-order-app-project/src/components/Cart/Checkout.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":33,"column":6,"nodeType":"85","endLine":33,"endColumn":13,"suggestions":"86"},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'cartCtx.items.length'. Either include it or remove the dependency array.","ArrayExpression",["89"],"no-global-assign","no-unsafe-negation",{"desc":"90","fix":"91"},"Update the dependencies array to be: [cartCtx.items.length, items]",